{
    "compilerOptions": {
        "module": "commonjs",
        "types": [
            "node",
            "express"
        ],
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "outDir": "build"
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}